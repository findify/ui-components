    "build:umd": "webpack --env.production --env.externals --output-path='./dist' --output-filename='findify-ui-components.js'",
    "build:umd:min": "webpack --env.production --optimize-minimize --output-path='./dist' --output-filename='findify-ui-components.min.js'",
    "check": "npm run build && npm run clean",
    "clean": "rimraf ./lib ./dist",
    "lint": "tslint --project .",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "postversion": "git push && git push --tags && git checkout develop && git merge master && git push && npm publish",
