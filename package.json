  "license": "MIT",
  "main": "./lib/index.min.js",
  "scripts": {
    "start": "webpack-dashboard -- ./node_modules/.bin/webpack-dev-server",
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "tsc --project . && babel ./__lib-es6__ --out-dir ./lib && rimraf ./__lib-es6__",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.ts dist/findify-ui-components.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.ts dist/findify-ui-components.min.js",
    "check": "npm run build && npm run lint && npm run clean",
    "clean": "rimraf ./lib ./dist",
    "lint": "tslint --project .",
    "postinstall": "./node_modules/.bin/webpack --env.dll",
    "postpublish": "npm run clean",
    "postversion": "git push && git push --tags && git checkout develop && git merge master && git push && npm publish",
    "preversion": "npm run check && git checkout master && git merge develop"
  }
